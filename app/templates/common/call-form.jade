form.call-form(action="ajax.php", method="post", name="call-form", id="form")
    fieldset.call-form__inner
        h2.call-form__inner-title Заказать звонок
        div.call-form__inner-elem.clearfix
            div.call-form__inner-text
                label.call-form__inner-label(for="call-name") Ваше имя
            div.call-form__inner-item
                input.call-form__inner-input.form-input(type="text", name="name", placeholder="Константин",id="call-name", data-validation="text")
        div.call-form__inner-elem.clearfix
            div.call-form__inner-text
                label.call-form__inner-label(for="call-phone") Телефон
            div.call-form__inner-item
                input.call-form__inner-input.form-input(type="tel", name="phone", id="call-phone", placeholder="+7 (912) 000-00-00", data-validation="phone")
        div.call-form__inner-elem.clearfix
            div.call-form__inner-select
                label.call-form__inner-label Дата звонка
            div.call-form__inner-month
                div.call-form__wrap-select
                    select.form-select
                        option(value="1") 01
                        option(value="2") 02
                        option(value="3") 03
                        option(value="4") 04
                        option(value="5") 05
                        option(value="6") 06
                        option(value="7") 07
                        option(value="8") 08
                        option(value="9") 09
                        option(value="10") 10
                        option(value="11") 11
                        option(value="12") 12
            div.call-form__inner-day
                div.call-form__wrap-select
                    select.form-select
                        option(value="1") 01
                        option(value="2") 02
                        option(value="3") 03
                        option(value="4") 04
                        option(value="5") 05
                        option(value="6") 06
                        option(value="7") 07
                        option(value="8") 08
                        option(value="9") 09
                        option(value="10") 10
