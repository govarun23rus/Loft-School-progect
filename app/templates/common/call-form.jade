        div.call-form__inner-elem.clearfix
            textarea.call-form__inner-input.call-form__inner-textarea(name="message", placeholder="Комментарий", data-validation="text")
        div.call-form__inner-elem.clearfix
            div.left-btn
                input.form-btn(type="submit", value="Отправить")
            div.right-btn
                input.form-btn(type="reset", value="Очистить" id="reset")
